<main class="signForm mdl-grid">
  <!-- UserName -->
   <div class="mdl-cell--6-col regularLogin">
     <div class="usernameRow">
       <label for="usernameInput"> Username </label><br>
       <input  id="usernameInput" type="text" name="usernameInput">
     </div>
   <!-- Password -->
     <div class="passwordRow">
       <label for="passwordInput"> Password </label><br>
       <input  id="passwordInput" type="password" name="passwordInput">
     </div>

     <button class ='mdc-button mdc-button--raised submitBtn loginBtn'> Log In </button>
   </div>

   <div class="mdl-cell--6-col">
     <div class="socialLoginBox">
       <p class="socialLoginIntro">
         <span style="margin-left: -140px"></span>Or Log in with..</p>
       <button id="facebookLoginBtn" class="mdc-button mdc-button--raised socialLoginBtn">
         <img src="images/facebook.png" class="smIcon">
       </button>
       <button id="googleLoginBtn" class="mdc-button mdc-button--raised socialLoginBtn">
         <img src="images/google-plus.png" class="smIcon">
       </button>
     </div>
   </div>
</main>

