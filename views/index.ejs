<!-- <main class="mdl-layout__content"> -->
  <div class="screen__posts mdl-grid">

    <!-- Tinder img go here -->
    <div id="viewport" style="margin-top: -60px; margin-bottom:15px; height:55vh; ">
        <ul class="stack">
          <%userArr2.forEach((user) => {%>
            <li style="background-image: url('<%=user.photoUrl%>')" onmousedown="clickOrDrag('/profile/<%=user._id%>')"></li>
          <%})%>
        </ul>
    </div>
    <nav class="demo-nav mdl-cell mdl-cell--12-col" style="margin-bottom: 50px;">
      <div class="section-spacer"></div>
      <a href="/match" class="demo-nav__button" title="show more" style="color: gray; font-size: 1.2em;">
        Expand
        <button class="mdl-button mdl-js-button mdl-js-ripple-effect mdl-button--icon">
          <i class="material-icons" role="presentation" style="font-size: 1.2em;">arrow_forward</i>
        </button>
      </a>
    </nav>
    <!-- <div class="mdl-card tinder-gif mdl-cell mdl-cell--12-col">
      <div class="mdl-card__media">
      </div>
    </div> -->

    <!--Grindr like view-->
    <div class="mdl-grid gridViewContainer" style="padding: 0; margin: 0 auto;">
    <%userArr.slice(0,4).forEach((user)=>{%>
    <div class="mdl-cell mdl-cell mdl-cell--3-col mdl-cell--2-col-tablet mdl-cell--2-col-phone mdl-cell--middle">
      <a href="/profile/<%=user._id%>">
        <div class="mdl-card__media profilePic" style="background-image: url('<%=user.photoUrl%>')">
        </div>
      </a>
      <div class="mdl-card__supporting-text meta mdl-color-text--grey-600 headline">
        <div class="cookieLogo"></div>
        <div>
          <%if(user.name.length>8){%>
            <strong><%=user.name.slice(0,8)+"..."%></strong>
          <%}else{%>
            <strong><%=user.name%></strong>
          <%}%>
          <span><%=user.age%>, <img src="images/genderIcon.png" class="gendericon"> <%=user.gender%></span>
        </div>
      </div>
    </div>
    <%});%>
    </div>
    <nav class="demo-nav mdl-cell mdl-cell--12-col">
      <div class="section-spacer"></div>
      <a href="/list" class="demo-nav__button" title="show more" style="color: gray; font-size: 1.2em;">
        Expand
        <button class="mdl-button mdl-js-button mdl-js-ripple-effect mdl-button--icon">
          <i class="material-icons" role="presentation" style="font-size: 1.2em;">arrow_forward</i>
        </button>
      </a>
    </nav>

    <!-- Thread -->
    <div class="mdl-card amazing mdl-cell mdl-cell--12-col">
      <div class="mdl-card__title mdl-color-text--grey-50" style="background : linear-gradient(45deg, #fd5c63, #8B0000); height: 250px;">
        <h3 class="quote"><a href="entry.html">I couldn’t take any pictures but this was an amazing thing…</a></h3>
      </div>
      <div class="mdl-card__supporting-text mdl-color-text--grey-600" style="box-shadow: 0 0 2px rgba(105,105,105,.5), 1px 1px 1px rgba(105,105,105,.5);">
        Enim labore aliqua consequat ut quis ad occaecat aliquip incididunt. Sunt nulla eu enim irure enim nostrud aliqua consectetur ad consectetur sunt ullamco officia. Ex officia laborum et consequat duis.
      </div>
    </div>
    <nav class="demo-nav mdl-cell mdl-cell--12-col" style="margin-top: 0;">
      <div class="section-spacer"></div>
      <a href="/ads" class="demo-nav__button" title="show more" style="color: gray; font-size: 1.2em;">
        Expand
        <button class="mdl-button mdl-js-button mdl-js-ripple-effect mdl-button--icon">
          <i class="material-icons" role="presentation" style="font-size: 1.2em;">arrow_forward</i>
        </button>
      </a>
    </nav>

  </div>
